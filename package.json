{
  "name": "rclone-rc",
  "version": "0.1.7",
  "description": "A fully typed TypeScript API client for Rclone RC",
  "homepage": "https://github.com/CodyAdam/rclone-rc",
  "repository": {
    "type": "git",
    "url": "https://github.com/CodyAdam/rclone-rc.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "openapi": "tsx src/generate-openapi.ts",
    "status": "tsx src/generate-current-status-md.ts",
    "preCommit": "pnpm run lint && pnpm run format && pnpm run test && pnpm run openapi && pnpm run status"
  },
  "keywords": [
    "rclone",
    "rclone-rc",
    "api-client",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "tsx": "^4.19.3",
    "typescript": "^5.4.3",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "@ts-rest/core": "^3.52.1",
    "@ts-rest/open-api": "^3.52.1",
    "zod": "^3.22.4"
  }
}
